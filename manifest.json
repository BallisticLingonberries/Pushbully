{
	"name": "Pushbully: Bulk Delete Pushes",
	"short_name": "Pushbully",
	"version": "0.9.8",
	"manifest_version": 3,
	"description": "Pushbully adds a \"Delete All Pushes\" button on your Pushbullet page.",
	"icons": {
        "16": "images/icon_16.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
    },
	"author":"Karl Tatom",
	"homepage_url": "https://www.pushbullet.com/",
	"minimum_chrome_version": "26",
	"permissions": ["https://www.pushbullet.com/*"],
	"content_scripts": [{
		"matches": ["https://www.pushbullet.com/*"],
		"css": ["inject/inject.css"],
		"js": ["inject/inject.js"],
		"all_frames": true
	}]
}