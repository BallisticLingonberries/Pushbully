{
    "name": "Pushbully: Bulk Delete Pushes",
    "short_name": "Pushbully",
    "version": "4.0",
    "manifest_version": 2,
    "description": "Pushbully allows you to easily delete pushes from Pushbullet (en masse!).",
    "icons": {
        "16": "common/images/icon_16.png",
        "48": "common/images/icon_48.png",
        "128": "common/images/icon_128.png"
    },
    "author": "Karl Tatom",
    "homepage_url": "https://chrome.google.com/webstore/detail/kamddpgihdoijjoekiambdmbngpgiaoi",
    "content_scripts": [
        {
            "matches": [ "*://*.pushbullet.com/*" ],
            "css": [ "common/inject.css" ],
            "js": [ "common/inject.js" ],
            "all_frames": true
        }
    ],
    "web_accessible_resources": [ "common/images/checkmark.png" ]
}
